vector={}

function vector:new(x,y)
 local prop={x=x,y=y}
 local meta={
 __index=vector,
 __add=function(v1,v2)
     return setmetatable(
     {x=v1.x+v2.x, 
      y=v1.y+v2.y}, 
      getmetatable(v1))
 end,
 }
 return setmetatable(prop,meta)
end

function vector:s()
 return self.x..","..self.y
end

